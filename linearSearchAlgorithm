//using c++ linear search algorithm implementation
#include <iostream>
using namespace std;
int main()
{
    int arr[] = {1,42,5,2,10,11};
    int size = sizeof(arr)/sizeof(arr[0]);
    int searchValue =10;
    for (int i=0;i<size;i++)
    {
        if (arr[i] == searchValue)
        {
            cout << "found search value in array " <<arr[i]<< " "<<i<<endl;
        }
    }
    return 0;
}
